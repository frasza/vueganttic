{
  "name": "vueganttic",
  "version": "0.1.0",
  "description": "Vue component library for Gantt charts",
  "type": "module",
  "main": "dist/vueganttic.umd.js",
  "module": "dist/vueganttic.es.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/vueganttic.es.js",
      "require": "./dist/vueganttic.umd.js"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.17",
    "tailwindcss": "^4.0.17",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vue-tsc": "^2.2.4"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "keywords": [
    "vue",
    "gantt",
    "chart",
    "component"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
